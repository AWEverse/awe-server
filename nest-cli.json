{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/awe-server/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/awe-server/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps",
  "projects": {
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "chat": {
      "type": "application",
      "root": "apps/chat",
      "entryFile": "main",
      "sourceRoot": "apps/chat/src",
      "compilerOptions": {
        "tsConfigPath": "apps/chat/tsconfig.app.json"
      }
    },
    "forum": {
      "type": "application",
      "root": "apps/forum",
      "entryFile": "main",
      "sourceRoot": "apps/forum/src",
      "compilerOptions": {
        "tsConfigPath": "apps/forum/tsconfig.app.json"
      }
    },
    "threads": {
      "type": "application",
      "root": "apps/threads",
      "entryFile": "main",
      "sourceRoot": "apps/threads/src",
      "compilerOptions": {
        "tsConfigPath": "apps/threads/tsconfig.app.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    },
    "video": {
      "type": "application",
      "root": "apps/video",
      "entryFile": "main",
      "sourceRoot": "apps/video/src",
      "compilerOptions": {
        "tsConfigPath": "apps/video/tsconfig.app.json"
      }
    }
  }
}