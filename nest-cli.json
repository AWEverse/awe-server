{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/chat/main/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/chat/main/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/chat/main",
  "projects": {
    "auth": {
      "type": "application",
      "root": "apps/chat/auth",
      "entryFile": "main",
      "sourceRoot": "apps/chat/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/chat/auth/tsconfig.app.json"
      }
    },
    "chat": {
      "type": "application",
      "root": "apps/chat",
      "entryFile": "main",
      "sourceRoot": "apps/chat/src",
      "compilerOptions": {
        "tsConfigPath": "apps/chat/tsconfig.app.json"
      }
    },
    "dialogs": {
      "type": "application",
      "root": "apps/dialogs",
      "entryFile": "main",
      "sourceRoot": "apps/dialogs/src",
      "compilerOptions": {
        "tsConfigPath": "apps/dialogs/tsconfig.app.json"
      }
    },
    "forum": {
      "type": "application",
      "root": "apps/forum",
      "entryFile": "main",
      "sourceRoot": "apps/forum/src",
      "compilerOptions": {
        "tsConfigPath": "apps/forum/tsconfig.app.json"
      }
    },
    "main": {
      "type": "application",
      "root": "apps/chat/main",
      "entryFile": "main",
      "sourceRoot": "apps/chat/main/src",
      "compilerOptions": {
        "tsConfigPath": "apps/chat/main/tsconfig.app.json"
      }
    },
    "media": {
      "type": "application",
      "root": "apps/media",
      "entryFile": "main",
      "sourceRoot": "apps/media/src",
      "compilerOptions": {
        "tsConfigPath": "apps/media/tsconfig.app.json"
      }
    }
  }
}
